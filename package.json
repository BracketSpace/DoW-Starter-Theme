{
  "name": "dow-starter-theme",
  "version": "1.0.0",
  "repository": "https://github.com/BracketSpace/DoW-Starter-Theme",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "start": "mp-scripts start --mode=development",
    "build": "mp-scripts build --mode=development",
    "postbuild": "tools generate theme-json",
    "build:production": "mp-scripts build --mode=production",
    "lint:js": "mp-scripts lint-js",
    "lint:scss": "mp-scripts lint-style",
    "fix:js": "mp-scripts lint-js --fix",
    "fix:scss": "mp-scripts lint-style --fix",
    "lint": "run-p \"lint:*\"",
    "fix": "run-p \"fix:*\""
  },
  "devDependencies": {
    "@babel/runtime": "^7.16.7",
    "@micropackage/responsive-embeds": "^1.0.0",
    "@micropackage/scripts": "^1.2.0",
    "@micropackage/vw": "^1.1.0",
    "@wordpress/icons": "^6.1.1",
    "classnames": "^2.3.1",
    "delegated-events": "^1.1.2",
    "fast-glob": "^3.2.11",
    "json-to-scss": "^1.6.2",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "object-fit-images": "^3.2.4",
    "postcss-object-fit-images": "^1.1.2",
    "postcss-prepend-selector": "^0.3.1",
    "tools": "file:./.tools"
  },
  "mpScriptsConfig": {
    "paths": {
      "src": "src/assets",
      "output": "build",
      "fonts": "new-fonts"
    }
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@svgr/webpack": "^6.2.0",
    "autoprefixer": "^10.4.2",
    "css-loader": "^6.5.1",
    "file-loader": "^6.2.0",
    "image-webpack-loader": "^8.1.0",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "postcss-loader": "^6.2.1",
    "sass": "^1.49.0",
    "sass-loader": "^12.4.0",
    "url-loader": "^4.1.1"
  }
}
