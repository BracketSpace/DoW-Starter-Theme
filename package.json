{
  "name": "dow-starter-theme",
  "version": "1.0.0",
  "repository": "https://github.com/BracketSpace/DoW-Starter-Theme",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "start": "mp-scripts start --mode=development",
    "prebuild": "tools generate sass-vars",
    "build": "mp-scripts build --mode=production",
    "postbuild": "tools generate theme-json",
    "build:dev": "mp-scripts build --mode=development",
    "lint:js": "mp-scripts lint-js",
    "lint:scss": "mp-scripts lint-style",
    "fix:js": "mp-scripts lint-js --fix",
    "fix:scss": "mp-scripts lint-style --fix",
    "lint": "run-p \"lint:*\"",
    "fix": "run-p \"fix:*\"",
    "sass-vars": "tools generate sass-vars",
    "theme-json": "tools generate theme-json"
  },
  "devDependencies": {
    "@babel/runtime": "^7.16.7",
    "@micropackage/responsive-embeds": "^1.0.0",
    "@micropackage/scripts": "^1.2.1",
    "@micropackage/vw": "^1.1.0",
    "@wordpress/icons": "^6.1.1",
    "autoprefixer": "^10.4.2",
    "classnames": "^2.3.1",
    "delegated-events": "^1.1.2",
    "fast-glob": "^3.2.11",
    "json-to-scss": "^1.6.2",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "object-fit-images": "^3.2.4",
    "postcss-object-fit-images": "^1.1.2",
    "postcss-prepend-selector": "^0.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "tools": "file:./.tools",
    "typescript": "^4.6.2"
  },
  "mpScriptsConfig": {
    "paths": {
      "src": "src/assets",
      "output": "build",
      "fonts": "new-fonts"
    }
  }
}
